<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	
	<context:annotation-config/>
	
	<bean id="television" class="com.scci.edu.di.autowired.Television"></bean>
	
	<bean id="lgSpeaker" class="com.scci.edu.di.autowired.LgSpeaker"></bean>
	
	<bean id="ssSpeaker" class="com.scci.edu.di.autowired.SsSpeaker"></bean>
	
	
	<bean id = "textEditor" class = "com.scci.edu.di.autowired.TextEditor">
	</bean>
   
	<bean id = "spellChecker" class = "com.scci.edu.di.autowired.SpellChecker">
	</bean>
	
</beans>
