<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Insert title here</title>
<script src='../js/menu.js'></script>
<script>

document.addEventListener("DOMContentLoaded", function(){
	var buildMenuBtn = document.getElementById("buildMenuBtn");
	buildMenuBtn.addEventListener("click", function(){
		var menuArr = [{season:"봄", food:"도다리쑥국"},
					{season:"여름", food:"수박"},
					{season:"가을", food:"전어구이"},
					{season:"겨울", food:"굴구이"}];
		var menu = document.getElementById("menu");
		for(var i=0; i<menuArr.length; i++){
			var liObj = document.createElement("li");
			liObj.innerText = menuArr[i].name;
			menu.appendChild(liObj);
		}
		foo();
	});
});
</script>
</head>
<body>

</body>
	<ul id="menu">
	</ul>
	<br>
	<button id="buildMenuBtn">메뉴 생성</button>
</html>